<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/atras"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/marca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calculador ITl"
        android:textColor="@android:color/holo_red_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.861"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.005" />

    <Button
        android:id="@+id/num9"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:text="9"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="248dp" />

    <Button
        android:id="@+id/num8"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:text="8"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="248dp" />

    <Button
        android:id="@+id/num7"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:text="7"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="248dp" />

    <Button
        android:id="@+id/num6"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:text="6"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="296dp" />

    <Button
        android:id="@+id/num5"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:text="5"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="296dp" />

    <Button
        android:id="@+id/num4"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:text="4"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="296dp" />

    <Button
        android:id="@+id/num3"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:text="3"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="344dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:text="2"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="344dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:text="1"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="344dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:text="0"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="392dp" />

    <Button
        android:id="@+id/punto"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:text="."
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="392dp" />

    <TextView
        android:id="@+id/Resultado"
        android:layout_width="329dp"
        android:layout_height="120dp"
        android:text="     Resultado"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColorLink="@color/colorPrimaryDark"
        android:textStyle="italic"
        android:visibility="visible"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="31dp" />

    <Button
        android:id="@+id/divicion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="248dp" />

    <Button
        android:id="@+id/multiplicacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="X"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="296dp" />

    <Button
        android:id="@+id/resta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:text="-"
        app:layout_constraintStart_toEndOf="@+id/num4"
        tools:layout_editor_absoluteY="344dp" />

    <Button
        android:id="@+id/suma"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="392dp" />

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="="
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="392dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_delete"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="164dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_save"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="164dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="64dp"
        android:layout_height="62dp"
        android:visibility="visible"
        app:srcCompat="@android:drawable/arrow_down_float"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="164dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="64dp"
        android:layout_height="63dp"
        app:srcCompat="@android:drawable/arrow_up_float"
        tools:layout_editor_absoluteX="245dp"
        tools:layout_editor_absoluteY="164dp" />

    <ImageButton
        android:id="@+id/limpiar"
        android:layout_width="64dp"
        android:layout_height="63dp"
        app:srcCompat="@android:drawable/ic_notification_clear_all"
        tools:layout_editor_absoluteX="322dp"
        tools:layout_editor_absoluteY="164dp" />

</android.support.constraint.ConstraintLayout>